<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Label</key>
	<string>com.jgdate.update</string>
	<key>LegacyTimers</key>
	<true/>
	<key>ProgramArguments</key>
	<array>
		<string>/bin/bash</string>
		<string>/usr/local/bin/run_date_replacements.sh</string>
	</array>
	<key>RunAtLoad</key>
	<false/>
	<key>StandardErrorPath</key>
	<string>/tmp/com.jgdate.update.stderr.log</string>
	<key>StandardOutPath</key>
	<string>/tmp/com.jgdate.update.stdout.log</string>
	<key>StartCalendarInterval</key>
	<dict>
		<key>Hour</key>
		<integer>0</integer>
		<key>Minute</key>
		<integer>25</integer>
	</dict>
	<key>StartInterval</key>
	<integer>3600</integer>
	<key>TimeOut</key>
	<integer>300</integer>
</dict>
</plist>
